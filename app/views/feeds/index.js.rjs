page.call 'itemBrowser.hideLoadingIndicator'

page.call "itemBrowser.setTotalItems", @feeds_count

@feeds.each_with_index do |feed, index|
  page.call 'itemBrowser.insertItem', dom_id(feed), params[:offset].to_i + index, 
    render_to_string(:partial => 'feed.html.erb', :locals => {:feed => feed})
end
