<% if current_user -%>
  <div id="navigation" class="navigation clearfix">
  	<ul class="extra">
      <li id="feedback"><%= link_to_remote("Leave Feedback", :url => new_feedback_path, :method => :get) %></li>
      <li><%= link_to("About", about_path) %></li>
      <li><%= link_to('My Info', edit_account_path(:id => current_user)) %></li>
      <li><%= link_to("Logout", logout_path) %></li>
    </ul>

  	<ul class="main clearfix">
      <li><%= link_to('Winnow Info', info_path, :class => tab_selected('about', "info")) %></li>
      <li><%= link_to('Items', feed_items_path, :class => tab_selected('feed_items')) %></li>
  	  <li><%= link_to('My Tags', tags_path, :class => tab_selected('tags', 'index')) %></li>
  	  <li><%= link_to('Public Tags', public_tags_path, :class => tab_selected('tags', 'public')) %></li>
      <li><%= link_to('Feeds', feeds_path, :class => tab_selected('feeds')) %></li>
      <% if is_admin? -%>
        <li><%= link_to('Admin', admin_path, :class => tab_selected('admin')) %></li>
      <% end -%>
      <% if help_path -%>
        <li><%= link_to('Help', help_path, :target => "_blank", :class => "help") %></li>
      <% end -%>
  	</ul>
	</div>
<% end -%>