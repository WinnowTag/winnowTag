if @full
  page.call 'itemBrowser.setFull', true
end
@tags.each do |tag|
  content = tag.user == current_user ? 
    render_to_string(:partial => 'tag.html.erb', :object => tag) :
    render_to_string(:partial => 'subscribed_tag.html.erb', :object => tag)
  
  page.call 'itemBrowser.insertItem', dom_id(tag), content
end
