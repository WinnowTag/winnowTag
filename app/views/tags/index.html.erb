<% content_for :head do -%>
  <% javascript_tag do -%>
		var itemBrowser;
		document.observe('dom:loaded', function() {
                  Content.setup();
			itemBrowser = new TagsItemBrowser('tags', 'content', { orders: {
			  asc:       ["name", "login", "state"],
			  desc:      ["subscriptions_count", "comments_count", "positive_count", "negative_count", "classifier_count", "last_classified", "last_trained"],
			  "default": "name"
			}});
		});
  <% end -%>
<% end -%>

<% content_for :header_controls do -%>
  <%= render :partial => 'header_controls' %>
<% end -%>
